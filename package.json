{
  "name": "decidim-superspaces",
  "version": "0.0.2",
  "description": "A participatory space to jointly manage assemblies and processes.",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "lint": "eslint -c .eslintrc.json --ext .js app/packs/",
    "lint-fix": "eslint -c .eslintrc.json --ext .js app/packs/ --fix",
    "markdownlint": "markdownlint '**/*.md'",
    "stylelint": "stylelint app/packs/**/*.scss",
    "stylelint-fix": "stylelint app/packs/**/*.scss --fix",
    "prettier": "prettier --check 'app/packs/**/*.scss'",
    "prettify": "prettier --write 'app/packs/**/*.scss'",
    "linthtml": "linthtml app/**/*.html.erb app/cells/**/*.erb"
  },
  "stylelint": {
    "extends": "@decidim/stylelint-config"
  },
  "prettier": "@decidim/prettier-config",
  "dependencies": {
    "@decidim/browserslist-config": "^0.29.2",
    "@decidim/webpacker": "^0.29.2"
  },
  "devDependencies": {
    "@decidim/dev": "^0.29.2",
    "@decidim/eslint-config": "^0.29.2",
    "@decidim/prettier-config": "^0.29.2",
    "@decidim/stylelint-config": "^0.29.2"
  },
  "browserslist": ["extends @decidim/browserslist-config"]
}
